					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"var expectedLDHashKey = data.expectedLDHashKey;",
							"if(expectedLDHashKey === undefined){",
							"    expectedLDHashKey = postman.getEnvironmentVariable(\"expectedLDHashKey\");",
							"}",
							"tests[\"2-CheckToken AzureFunction - Body matches string\"] = responseBody.has(expectedLDHashKey);",
							"tests[\"2-CheckToken AzureFunction - Status code is 200\"] = responseCode.code === 200;",
							"",
							"",
							"console.log(\"responseBody: \"+responseBody)",
							"console.log(\"expectedLDHashKey: \"+expectedLDHashKey);",
							""
						]
					}
				}
			],
